{
  "expo": {
    "name": "F1 Barcode Scanner",
    "slug": "f1-barcode-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow F1 Barcode Scanner to access your camera to scan barcodes."
        }
      ]
    ]
  }
}