{
  "buildCommand": "npm install && npx expo export:web",
  "outputDirectory": "web-build",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "camera=(*)"
        }
      ]
    }
  ]
}